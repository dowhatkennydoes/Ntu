# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database Configuration
DATABASE_URL=your_database_connection_string
PGVECTOR_EXTENSION_ENABLED=true

# Redis Configuration (for BullMQ)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
OLLAMA_BASE_URL=http://localhost:11434

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1h
REFRESH_TOKEN_EXPIRES_IN=7d

# File Upload Configuration
MAX_FILE_SIZE=104857600 # 100MB in bytes
ALLOWED_AUDIO_FORMATS=wav,mp3,m4a
UPLOAD_DIR=./uploads

# Whisper Configuration
WHISPER_MODEL=base
WHISPER_LANGUAGE=auto
WHISPER_TASK=transcribe

# Security Configuration
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000 # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring Configuration
SENTRY_DSN=your_sentry_dsn
LOGSNAG_TOKEN=your_logsnag_token
NODE_ENV=development

# API Configuration
API_VERSION=v1
API_BASE_URL=http://localhost:3000/api

# Feature Flags
ENABLE_VECTOR_SEARCH=true
ENABLE_AUDIO_TRANSCRIPTION=true
ENABLE_LLM_FALLBACKS=true
ENABLE_JOB_QUEUES=true
ENABLE_MONITORING=true

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/google/oauth/callback

# Google OAuth (Production)
GOOGLE_CLIENT_ID_PROD=your_production_google_client_id
GOOGLE_CLIENT_SECRET_PROD=your_production_google_client_secret
GOOGLE_REDIRECT_URI_PROD=https://your-domain.com/api/google/oauth/callback

# Google OAuth (Development)
GOOGLE_CLIENT_ID_DEV=your_development_google_client_id
GOOGLE_CLIENT_SECRET_DEV=your_development_google_client_secret
GOOGLE_REDIRECT_URI_DEV=http://localhost:3000/api/google/oauth/callback

# Meeting Bot Configuration
MEETING_BOT_EMAIL=yonder-bot@ntu.app
BOT_AVATAR_URL=https://ntu.app/bot-avatar.png
CHROME_EXTENSION_ID=yonder-meet-capture

# Development Configuration
DEBUG=true
LOG_LEVEL=info
ENABLE_SWAGGER=true 

